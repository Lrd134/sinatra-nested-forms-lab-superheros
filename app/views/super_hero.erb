  <label for="member1_power">Hero power:</label>
  <input type="text" id="member1_power" name="team[heros][][power]"/><br>
  <label for="member1_bio">Hero bio:</label>
  <input type="text" id="member1_bio" name="team[heros][][bio]"/><br>
  <label for="member2_name">Hero Name:</label>
  <input type="text" id="member2_name" name="team[heros][][name]"/><br>
  <label for="member2_power">Hero power:</label>
  <input type="text" id="member2_power" name="team[heros][][power]"/><br>
  <label for="member2_bio">Hero bio:</label>
  <input type="text" id="member2_bio" name="team[heros][][bio]"/><br>
  <label for="member3_name">Hero Name:</label>
  <input type="text" id="member3_name" name="team[heros][][name]"/><br>
  <label for="member3_power">Hero power:</label>
  <input type="text" id="member3_power" name="team[heros][][power]"/><br>
  <label for="member3_bio">Hero bio:</label>
  <input type="text" id="member3_bio" name="team[heros][][bio]"/><br>
  <input type="submit" id="submit"/>
  </form>
